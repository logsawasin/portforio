<%#= form_with model: @strategy, url: admins_strategies_path do |f| %>
<%= form_with model: [:admins, @strategy] do |f| %>
  <%= render 'layouts/error_messages', model: f.object %>
  
  <%= f.hidden_field :game_id, value: params[:game_id] %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  
  <h4>本文</h4>
  <%= f.text_field :body %>
  <br>
  <%= f.submit '投稿' %>
<% end %>
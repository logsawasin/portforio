<h3>攻略情報編集画面</h3>
    <%= form_with model: @strategy, url:admins_strategy_path, method: :patch do |f| %>
        <table>
            <tr><th>タイトル名</th><td><%= f.text_field :title %></td></tr>
            <tr><th>本文</th><td><%= f.text_field :body %></td></tr>
            <tr><th><%= f.submit "保存" %></th></tr>
        </table>
    <% end %>